package br.facisa.p3.ed.sortingAlgorithms;

public class Main {

	public static void main(String[] args) {
		double[] array = new double[4];
		array[0] = 10;
		array[1] = 11;
		array[2] = 5;
		array[3] = 18;
		array =  BubbleSort.sort(array);
		for (double x: array){
			System.out.println(x);
		}
	}

}
